{
  "name": "escroguard",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npm run generate && next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "generate": "POSTGRES_URL=\"$(node -p \"require('dotenv').config().parsed.POSTGRES_URL\")\" && kysely-codegen --log-level=debug --print --dialect=postgres --url=$POSTGRES_URL",
    "migrate:up": "TS_NODE_PROJECT=tsconfig.migrations.json ts-node library/db/migrations/index.ts up",
    "migrate:down": "TS_NODE_PROJECT=tsconfig.migrations.json ts-node library/db/migrations/index.ts down"
  },
  "dependencies": {
